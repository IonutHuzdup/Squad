<Window x:Class="Hotel_Reservation.ViewProfileWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hotel_Reservation"
        mc:Ignorable="d"
        Title="User Profile" Height="450" Width="800" Background="Black">
    <Grid>
        <Label x:Name="label" Foreground="White" Content="First Name:" HorizontalAlignment="Left" Margin="29,10,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label1" Foreground="White" Content="Last Name:" HorizontalAlignment="Left" Margin="35,41,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label2" Foreground="White" Content="Age: " HorizontalAlignment="Left" Margin="35,72,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
